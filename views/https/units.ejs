<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Home</title>
  <meta name="description" content="Home Page">
  <meta name="author" content="Luke Pierce">
  <link rel="stylesheet" href="../css/bootstrap.min.css">
  <link rel="stylesheet" href="../css/style.css">
</head>
<body>
  <nav>
    <% include ../partials/nav %>
  </nav>
  <div class="row">
    <div class="col-md-3" style="padding-left: 30px;">
      <% include ../partials/config %>
    </div>
    <div class="col-md-9">
    <h2>Units Configuration</h2>
    <form action="Output.html" method="POST" class="form-group" id="output">
      <div class="accordian">
        <div class="head" data-toggle="collapse" data-target="#outputs">
          Units
        </div>
        <div class="body collapse in" id="outputs">
          <div class="row">
            <div class="col-xs-3 col-xs-offset-3">
              <h5 class="label">Volume/Mass</h5>
            </div>
            <div class="col-xs-1">
              <h5 class="label">/</h5>
            </div>
            <div class="col-xs-3">
              <h5 class="label">Time</h5>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Mass Flow</h5>
            </div>
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Volume Flow</h5>
            </div>
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Energy Flow</h5>
            </div>
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>            
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Temperature</h5>
            </div>
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Pressure</h5>
            </div>
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Density</h5>
            </div>
            <div class="col-xs-3 col-xs-offset-1">
              <select class="form-control" id="measure{{index}}" name="analog_output{{index}}">
                  <option value="{{value}}">None</option>
                  <option value="{{value}}">Mass</option>
                  <option value="{{value}}">Volume</option>                  
              </select>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-3">
             <button type="button" class="btn btn-primary btn-block">SET</button>                                       
            </div>
           </div>
          <br>                                  
        </div>  
      </div>
      <br>
    </form>
    </div>
  </div>
  <section class="container container-small">
  </section>
  <footer>
  <% include ../partials/footer %> 
  </footer>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <script type="text/javascript" src="../js/script.js"></script>
</body>
</html>
