<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Home</title>
  <meta name="description" content="Home Page">
  <meta name="author" content="Luke Pierce">
  <link rel="stylesheet" href="../css/bootstrap.min.css">
  <link rel="stylesheet" href="../css/style.css">
</head>
<body>
  <nav>
    <% include ../partials/nav %>
  </nav>
  <div class="row">
    <div class="col-md-3" style="margin-left: 10px;">
      <% include ../partials/config %>
    </div>
    <div class="col-md-6">
    <h2>Diagnostics Level 2</h2>
    
    <form action="Output.html" method="POST" class="form-group" id="output">
      <div class="accordian">
        <div class="head" data-toggle="collapse" data-target="#outputs">
          4-20 ma Configuration
        </div>
        <div class="body collapse in" id="outputs">
          <div class="row">
            <div class="col-xs-3 col-xs-offset-2">
              <h5 class="label">Zero</h5>
            </div>
            <div class="col-xs-2">
              <h5 class="label">Fscale</h5>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">4-20(1)</h5>
            </div>
            <div class="col-xs-3">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
            <div class="col-xs-3">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">4-20(2)</h5>
            </div>
            <div class="col-xs-3">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
            <div class="col-xs-3">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">4-20(3)</h5>
            </div>
            <div class="col-xs-3">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
            <div class="col-xs-3">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
          </div>
        </div>           
      </div>
    </form>

    <form action="Output.html" method="POST" class="form-group" id="output">
      <div class="accordian">
        <div class="head" data-toggle="collapse" data-target="#outputsTwo">
          Factory Configuration
        </div>
        <div class="body collapse in" id="outputsTwo">
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Reynolds Corr.</h5>
            </div>
            <div class="col-xs-4">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
            <div class="col-xs-2">
              <h5 class="label">Roughness</h5>
            </div>
            <div class="col-xs-4">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Gain Control</h5>
            </div>
            <div class="col-xs-4">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
            <div class="col-xs-2">
              <h5 class="label">Min Delta H</h5>
            </div>
            <div class="col-xs-4">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Filter Control</h5>
            </div>
            <div class="col-xs-4">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
            <div class="col-xs-2">
              <h5 class="label">Init Displ. (sec)</h5>
            </div>
            <div class="col-xs-4">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">High Pass Filter</h5>
            </div>
            <div class="col-xs-4">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
          </div>
          <br>
          <div class="row bottom-buffer-lg">
            <div class="col-xs-2">
              <h5 class="label">Meter Type</h5>
            </div>
            <div class="col-xs-4">
              <input class="form-control" type="text" value="0" mb name="mA20{{index}}" id="mA20{{index}}"/>
            </div>
          </div>                              
        </div>                             
      </div>
    </form>

    </div>
  </div>
  <section class="container container-small">
  </section>
  <footer>
  <% include ../partials/footer %> 
  </footer>
