    <script>
    $(document).ready(function(){
      $(".meterConfig").css("display", "none");
      $('#selectBox').on('change', function() {
       if(this.value == 0){
         $(".meterConfig").css("display", "none");
         $(".meterDiag").css("display", "inline");
       }
       else if(this.value == 1){
         $(".meterConfig").css("display", "inline");
         $(".meterDiag").css("display", "none");
       }
      });
    });
    </script>
